
  <h1>Shortened URLs</h1>
  <table>
  <tr>
  <th>Description</th>
  <th>Shortened Url</th>
  <th>Clicks</th>
  <th>Favorite?</th>
  </tr>
    <% @urls.reverse.each do |url| %>
  <tr>
  <td><%= url.description %></td>
  <td><a href="/<%=url.shortened_url%>">http://<%= url.shortened_url %></a></td>
  <td>(<%= url.clicks %>)</td>
  <td>
    <form action="/favorite_url" method="post">
      <input type="hidden" name="favorite[url_id]"  value="<%= url.id %>">
      <input type="submit" value="OMG MyFav">
    </form>

  </td>
  </tr>
  <% end %>
  </table>


